package chapter02;

public class TestChapter2 {
	public static void main(String[] args) {
		String name = "감자바";
		int age = 25;
		String tel1="010", tel2="123", tel3="4567";
		
		System.out.println("이름: "+name+"나이: "+age+"전화: "+tel1+tel2+tel3);
		// System.out.print();
		System.out.printf("이름: %s \n나이: %d \n 전화: %s-%s-%s",name,age,tel1,tel2,tel3);
	}
}
